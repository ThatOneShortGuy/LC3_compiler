int num = 4
char c = 'a'

char ch = c + num

int arr = [1,2,3,4,5]
string str = "hello world"
arr++;
--arr;
str++;
--str;

if ch > 'd':
    ch = 'g'
elif ch == 'd':
    ch = 's'
else:
    ch = 'l'

arr[2] = ch
for (int i = 0; i < arr.length, i++)
    arr[i] = arr[i] + arr[i+1]

while *&str != '\0':
    *&str = arr + 65
    &str++
